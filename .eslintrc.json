{
    "extends": "airbnb",
    "rules": {
        "react/jsx-no-undef": ["error", { "allowGlobals": true }],
        "no-else-return": ["error", { "allowElseIf": true}]
    },
    "env": {
        "browser": true
    },
    "globals": {
        "React": "readonly",
        "ReactDOM": "readonly",
        "PropTypes": "readonly",
        "Navbar": "readonly",
        "PetGenerator": "readonly",
        "CurrentPet": "readonly",
        "PetDisplay": "readonly",
        "WeatherDisplay": "readonly",
        "StatDisplay": "readonly",
        "Stat": "readonly",
        "Login": "readonly",
        "CreateAcct": "readonly",
        "Play": "readonly"
    }   
}